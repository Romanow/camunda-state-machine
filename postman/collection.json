	"item": [
		{
			"name": "Create Calculation",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Create Calculation\", function() {",
							"    pm.response.to.have.status(201)",
							"    var location = pm.response.headers.get(\"Location\")",
							"    pm.expect(location).to.not.empty;",
							"    var l = location.split(\"/\").pop()",
							"    pm.environment.set(\"calculationUid\", l)",
							"})"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"name\": \"{{$randomProductName}}\",\n    \"periods\": [\n        {\n            \"startDate\": \"2022-10-01\",\n            \"endDate\": \"2022-12-01\",\n            \"mark\": \"M1\",\n            \"serialNumber\": 1\n        }\n    ]\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{baseUrl}}/api/v1/cashflow/calculation",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						"api",
						"v1",
						"cashflow",
						"calculation"
					]
				}
			},
			"response": []
		},
		{
