			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Etl Process Completed\", function() {",
							"    pm.response.to.have.status(200)",
							"})"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"solveId\": \"{{calculationUid}}\",\n    \"status\": \"UPLOADED\",\n    \"aggReportTableName\": \"cashflow.public.data\",\n    \"calculationParametersTables\": {}\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
